<form method="post" action="{{ path('app_rechargement_delete', {'id': rechargement.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ rechargement.id) }}">
    <button class="btn btn-danger mt-3" > <span class="mb-1 mx-2" data-feather="trash-2"></span>Supprimer</button>
</form>
